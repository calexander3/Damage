    <authentication mode="Forms">
      <forms loginUrl="~/Account/Login" timeout="10080" slidingExpiration="true" requireSSL="true" cookieless="UseCookies" />
    </authentication>
    <pages>
      <namespaces>
        <add namespace="System.Web.Helpers" />
